#!/usr/bin/env python

from time import sleep
from RosAriaDriver_package.drive import RosAriaDriver

robot=RosAriaDriver('/PIONIER2')
robot.GripperDown();
sleep(5);
robot.GripperOpen();
sleep(5);
robot.GripperUp();
sleep(5);
robot.GripperClose();
sleep(5);
robot.goto(-3);
sleep(5);
robot.goto(3);
robot.rotate(90)
    
